                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="cslCreatorMap">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="map" maxOccurs="unbounded" minOccurs="0">
