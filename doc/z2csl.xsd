            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="cslVars">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="itemTypes">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="type" maxOccurs="unbounded" minOccurs="0">
